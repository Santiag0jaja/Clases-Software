Algoritmo celsius_a_fahrenheit
	Escribir "Ingrese su nombre a continuacion: ";
	Leer nombre_user; 
	Escribir "Hola, ", nombre_user;
	Escribir "A continuacion, ingrese los celsius a convertir:";
	Leer celsius;
	fahrenheit <- (celsius * 9/5) + 32;
	Escribir celsius, " celsius son: ", fahrenheit, " fahrenheit.";
	Escribir "Hasta luego, ", nombre_user;

FinAlgoritmo
