Algoritmo operadores_de_comparacion
	Escribir "Operador de igualdad";
	Escribir "10 = 10 ", 10 = 10;
	Escribir "-----------------------------------------------------------";
	Escribir "Operador de mayor que";
	Escribir "10 > 8?: ", 10 > 8;
	Escribir "-----------------------------------------------------------";
	Escribir "Operador de menor que";
	Escribir "38 < 20?: ", 38 < 20;
	Escribir "-----------------------------------------------------------";
	Escribir "Operador de mayor o igual que";
	Escribir "20 => 20?: ", 20 >= 20;
	Escribir "-----------------------------------------------------------";
	Escribir "Operador de menor o igual que";
	Escribir "38 <= 20?: ", 38 <= 20;
	Escribir "-----------------------------------------------------------";
	Escribir "Operador de diferente"; 
	Escribir "20 <> 30?: ", 20 <> 30;
	Escribir "-----------------------------------------------------------";
	
	// Solo se pueden comparar variables del mismo tipo :D
	// Escribir "Hola Mundo <> 34?: ", "Hola Mundo" <> 34;
FinAlgoritmo
